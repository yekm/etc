#defaults.pcm.rate_converter "speexrate_medium"
#defaults.pcm.rate_converter "speexrate_best"

pcm.!default {
    type pulse
}
ctl.!default {
    type pulse
}

pcm.rawjack {
    type jack
    playback_ports {
        0 "Calf Studio Gear:Gate In #1"
        1 "Calf Studio Gear:Gate In #2"
    }
    capture_ports {
        0 "inputs:mono In #1"
    }
}

#pcm.!default {
pcm.j {
    type plug
    slave { pcm "rawjack" }
    hint {
 	description "JACK Audio Connection Kit"
    }
}

