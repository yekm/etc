pcm.mydmix {
    type dmix
    ipc_key 5678293
    ipc_perm 0660
    ipc_gid audio

    slave {
        channels 4 # make 6 or 5.1 channel
        pcm
        {
            type hw
            card "PCH"
        }

        period_size 1024
        buffer_size 8192
    }
}


pcm.myttable {
    type route
    slave.pcm jack
    slave.channels 4
    ttable.0.0 1
    ttable.1.1 1
    ttable.0.2 1
    ttable.1.3 1
}

pcm.duplicate {
    type plug
    slave.pcm "mydmix"
    slave.channels 4
    route_policy duplicate
}

pcm.!default {
    type plug
    slave.pcm {
        @func getenv
        vars [ ALSA_PLUG ]
        default "duplicate"
    }
}

ctl.mixer0 {
    type hw
    card 1
}

pcm.jackplugmpd {
    type plug
    slave {
            pcm "jackmpd"
    }
}

pcm.jack {
    type jack
    playback_ports {
        0 onboard_p:playback_1
        1 onboard_p:playback_2
        2 onboard_p:playback_3
        3 onboard_p:playback_4
    }
    capture_ports {
        0 onboard_c:capture_1
        1 onboard_c:capture_2
    }
}

pcm.jackmpd {
    type jack
    playback_ports {
#        0 system:playback_1
#        1 system:playback_2
        0 eq12:in_l
        1 eq12:in_r
    }
    capture_ports {
        0 system:capture_1
        1 system:capture_2
    }
}

