! scrollbar style - rxvt (default), plain (most compact), next, or xterm
URxvt.scrollstyle: rxvt

! URxvt.scrollBar: false

! URxvt.font: 9x15

!URxvt.font: xft:Droid sans mono slashed:size=10

!Xft.dpi: 96
!Xft.antialias: 0
!Xft.hinting:    1
!Xft.hintstyle:  hintfull
!Xft.rgba:       rgb
xterm*faceName: 
!xterm*faceName: -*-montecarlo-medium-r-normal-*-11-*-*-*-*-*-*-*
xterm*faceSize: 24
Xmerm*vt100*faceSize: 24

URxvt*font: -*-terminus-medium-*-*-*-14-*-*-*-*-*-*-*
URxvt*boldFont: -*-terminus-bold-*-*-*-14-*-*-*-*-*-*-*
!URxvt*boldFont: -*-terminus-medium-r-normal-*-14-*-*-*-*-*-iso10646-1

!URxvt.font: xft:Droid Sans Mono Slashed:size=10:antialias=true:hinting=true
!URxvt.font: xft:monaco:size=10
!URxvt*font: xft:Liberation Mono:pixelsize=12:antialias=false:hinting=false
!URxvt*boldFont: xft:Liberation Mono:bold:pixelsize=12:antialias=false:hinting=false

!URxvt.scrollBar: off

urxvt*perl-lib:         /usr/lib/urxvt/perl/
urxvt*perl-ext-common:  default,matcher,tabbed
urxvt*matcher.button:   1
urxvt*urlLauncher:      /usr/bin/google-chrome
URxvt*saveLines:        50000

!URxvt*font: xft:DejaVu Sans Mono-10


